---
const links = ['Home', 'Services', 'Gallery', 'About', 'FAQ']
---

<div class='hidden md:flex md:items-center md:justify-center md:flex-grow-1'>
	<!-- Current: "bg-gray-900 text-white", Default: "text-gray-300 hover:bg-gray-700 hover:text-white" -->

	{
		links.map(link => (
			<a
				href='#'
				class=' text-dark-gray rounded-md mx-2 px-3 py-2 text-sm font-medium'
				aria-current='page'
			>
				{link}
			</a>
		))
	}
</div>
