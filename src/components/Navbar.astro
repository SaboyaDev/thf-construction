---
import NavLinks from "./NavLinks.astro";
import CTAButton from "./CallToActionBtn.astro";
import HamburgerBtn from "./HamburgerBtn.astro";
import Logo from "./Logo.astro";
import NavbarLayout from "../layouts/NavbarLayout.astro";
---

<NavbarLayout>
  <div class="flex h-[100px] items-center gap-3 lg:justify-between">
    <Logo />
    <NavLinks />
    <CTAButton
      content="Start Your Project"
      icon="arrow_right_alt"
      color="bg-main"
      hidden="hidden"
    />
    <HamburgerBtn />
  </div>
</NavbarLayout>
